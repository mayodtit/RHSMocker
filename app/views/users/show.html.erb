<p id="notice"><%= notice %></p>

<%= @user.firstName %>&nbsp;<%= @user.lastName %><br/>
<%= @user.install_id %>


<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="readingList">
	<thead>
		<th>Headline</th>
		<th>Date Read</th>
		<th>Date Dismissed</th>
		<th>Read Later Date</th>
		<th>Read Later Count</th>
	</thead>
	<tbody>
	<% @user.readContent.each do | readingItem | %>
  		<tr> 
  			<td> <%= readingItem.content.headline %> </td>
  			<td> <%= readingItem.read_date %> </td>
  			<td> <%= readingItem.dismiss_date %> </td>
  			<td> <%= readingItem.read_later_date %> </td>
  			<td> <%= readingItem.read_later_count %> </td>
  		</tr>
  	<% end %>
  </tbody>
</table>

<%= link_to 'Reset All Content', reset_content_url(:id => @user.id) %>
